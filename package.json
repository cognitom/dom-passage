{
  "name": "dominiq",
  "version": "0.7.0",
  "description": "[WIP] JavaScript utilities and web components for extracting the data from DOM",
  "module": "lib/index.js",
  "main": "index.js",
  "files": [
    "index.js",
    "tags.js",
    "lib"
  ],
  "scripts": {
    "build": "npm run prettier && npm-run-all rollup-*",
    "rollup-main": "rollup -c",
    "rollup-tags": "rollup -c tags/rollup.config.js",
    "rollup-demo": "rollup -c demo/basic-app/rollup.config.js",
    "rollup-demo2": "rollup -c demo/input-proxy/rollup.config.js",
    "demo": "npm-run-all --parallel watch-* live-server",
    "watch-main": "rollup -w -c",
    "watch-tags": "rollup -w -c tags/rollup.config.js",
    "watch-demo": "rollup -w -c demo/basic-app/rollup.config.js",
    "watch-demo2": "rollup -w -c demo/input-proxy/rollup.config.js",
    "live-server": "live-server demo/ --no-browser",
    "test": "NODE_ENV=test jest --no-cache",
    "prettier": "prettier --write {lib,tags,test}/**/*.js demo/**/{main,view}.js demo/lib/*.js rollup.config.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cognitom/dominiq.git"
  },
  "keywords": [],
  "author": "Tsutomu Kawamura",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cognitom/dominiq/issues"
  },
  "homepage": "https://github.com/cognitom/dominiq#readme",
  "dependencies": {
    "fafgag": "^0.3.1",
    "zen-observable": "^0.8.8"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "esm": "^3.0.33",
    "jest": "^22.4.3",
    "lit-html": "^0.10.0",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "rollup": "^0.58.2",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "babel": {
    "presets": "env"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/*.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!fafgag).+\\.js$"
    ]
  },
  "prettier": {
    "semi": false
  }
}
