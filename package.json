{
  "name": "dominiq",
  "version": "0.3.1",
  "description": "[WIP] JavaScript utilities and web components for extracting the data from DOM",
  "module": "lib/index.js",
  "main": "index.js",
  "files": [
    "index.js",
    "tags.js",
    "observable-polyfill.js",
    "lib"
  ],
  "scripts": {
    "build": "npm run prettier && npm-run-all rollup-*",
    "rollup-main": "rollup -c",
    "rollup-tags": "rollup -c tags/rollup.config.js",
    "rollup-polyfill": "rollup -c polyfill/rollup.config.js && cp observable-polyfill.js demo/lib/",
    "rollup-demo": "rollup -c demo/basic-app/rollup.config.js",
    "demo": "npm-run-all --parallel watch-* live-server",
    "watch-main": "rollup -w -c",
    "watch-tags": "rollup -w -c tags/rollup.config.js",
    "watch-demo": "rollup -w -c demo/basic-app/rollup.config.js",
    "live-server": "live-server demo/ --no-browser",
    "test": "jest",
    "prettier": "prettier --write {lib,tags,polyfill,test}/**/*.js demo/{main,view,lib}.js rollup.config.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cognitom/dominiq.git"
  },
  "keywords": [],
  "author": "Tsutomu Kawamura",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cognitom/dominiq/issues"
  },
  "homepage": "https://github.com/cognitom/dominiq#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^22.4.3",
    "lit-html": "^0.9.0",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.1",
    "rollup": "^0.58.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "zen-observable": "^0.8.8"
  },
  "babel": {
    "presets": "env"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/*.js"
    ],
    "setupFiles": [
      "./test/lib/setup.js"
    ]
  },
  "prettier": {
    "semi": false
  }
}
